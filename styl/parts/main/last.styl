#last
    width 100%
    padding: 115px 0 70px
    background-image url(../img/last_bg.jpg)
    background-repeat repeat
    background-position: center
    background-size: cover
.last_wrapper
    width 100%
    max-width 749px
    border-radius: 10px
    padding 51px 10px 64px
    margin 0 auto
    background-color #fff
    background-image url(../img/mail_bg.jpg)
    text-align: center
h3.last_subtitle
    font-size: 30px;
    font-family: "TrajanPro3Regular", serif;
    color: rgb(0, 0, 0);
    line-height: 1.567;
    margin 0 auto 2px
    width 100%
    max-width 385px
    & + p
        font-size: 20px
        font-family: "CenturyGothicBold", sans-serif;
        color: rgb(116, 100, 59);
        line-height: 1.3;
h4.last_subtitle
    font-size: 18px;
    font-family: "TrajanPro3Bold", serif;
    color: rgb(0, 0, 0);
    line-height: 1.35;
    margin 39px 0 23px
    text-transform: uppercase