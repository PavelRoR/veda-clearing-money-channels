*
	margin 0
	padding 0
	outline none
body
	font-size: 16px
	font-family: 'CenturyGothic', sans-serif
a
	color: #fefefe
	&:focus
		text-decoration: none
		outline: 0
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight: normal
button
	border none
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #fff
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	background-image: linear-gradient( 0deg, rgb(128,110,63) 15%, rgb(226,218,155) 100%);
	box-shadow: inset 0 0 5px rgba(#ffffbe, .24), 0px 4px 0px 0px rgba(142, 123, 74, 0.98);
	width 100%
	max-width: 300px;
	height: 65px;
	line-height: 65px;
	font-size: 15px;
	font-family: "CenturyGothicBold", sans-serif;
	color: rgb(254, 254, 253);
	text-transform: uppercase;
	text-shadow: 0px 0px 4.9px rgba(19, 16, 9, 0.5);
	text-align: center
	border-radius: 32.5px
	margin 0 auto
	&:focus
		color rgb(254, 254, 253);
		text-decoration: none
	&:hover
		background-image: linear-gradient( 180deg, rgb(128,110,63) 15%, rgb(226,218,155) 100%);
		color rgb(254, 254, 253);
		text-decoration: none
